# Copy this file to `.env` and set your local secrets.
# NOTE: Spring Boot does NOT load .env automatically. Use your shell/IDE to load these
# OR install a tool like direnv / IntelliJ EnvFile plugin. This file is for reference only.

# App environment selector used by EnvConfig (dev|prod)
APP_ENV=dev

# Cognito app client secret (do NOT commit your real value)
COGNITO_CLIENT_SECRET=changeme

# Optional: local server port (Spring Boot falls back to 5000 via application.yml)
SERVER_PORT=5000

